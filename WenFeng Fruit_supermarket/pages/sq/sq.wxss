/* pages/sq/sq.wxss */
.head{
  position: relative;
  width: 100vw;
  height: 150rpx;
  background-color: rgb(255,120,0);
  display: flex;
  justify-content: center;
}
.head span{
    color: white;
    font-size: 32rpx;
    position: absolute;
    bottom: 30rpx;
}
.header{
  height: 500rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
image {
  width: 200rpx;
  height: 200rpx;
}

.content {
  margin-left: 50rpx;
  margin-bottom: 90rpx;
}

.content text {
  display: block;
  color: #9d9d9d;
  margin-top: 40rpx;
}

.bottom {
  border-radius: 80rpx;
  margin: 90rpx 50rpx;
  font-size: 35rpx;
}